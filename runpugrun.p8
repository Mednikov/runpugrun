function _init()
  cls()
  palt(0, false)
  palt(5, true)
end

function _update()
  -- runpug()
  pughandle()
  -- pugmoving()
  -- test()
end

function _draw()

  -- clear the screen
  rectfill(0,0, 128,128, 7)

 -- draw the map
  map(0,0,0,0,16,16)

  -- draw the pug
  spr(pug.sprite, pug.x, pug.y, 1, 1, pug.flip)

  print(pug.sprite, 5 , 5, 14)
  print(pug.y, 15, 5, 14)
  print(jumptop, 25, 5, 14)
  print(pug.moving, 5 , 12, 14)
  print(pug.jumping, 5 , 19, 14)

