pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

pug = {
  x = 30,
  y = 90,
  speed = 2,

  startspr = 4,
  endspr = 7,

  moving = false,
  move = {
    startspr = 20,
    endspr = 23
  },

  jumping = false,
  jump = {
    max = 20
  },

  flip = false
}

pug.sprite = pug.startspr

function pugjump(pugy)
  jumptop = pugy - pug.jump.max
  if  pugy > jumptop  then pug.y -= pug.speed end
  pug.sprite = pug.move.startspr
end  

function puganimate(startsprite, endsprite)
  pug.sprite += 0.125
  if pug.sprite < endsprite then pug.sprite = endsprite end
  if pug.sprite > endsprite then
    pug.sprite = startsprite
  end
end

function pughandle()
  pug.moving = false
  pug.jumping = false
  if btn(0) then
    pug.moving = true
    pug.flip = true
    pug.x -= pug.speed
    puganimate(pug.move.startspr, pug.move.endspr)
  end
  if btn(1) then
    pug.moving = true
    pug.flip = false
    pug.x += pug.speed
    puganimate(pug.move.startspr, pug.move.endspr)
  end
  if not pug.moving then
    puganimate(pug.startspr, pug.endspr)
  end
  if btn(2) then
    pug.jumping = true
    pugjump(pug.y)
  end
end

